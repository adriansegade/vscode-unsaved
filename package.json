{
  "name": "unsaved",
  "displayName": "YOU HAVE UNSAVED FILES",
  "description": "Change status bar color when you have some unsaved files so you can actually notice it.",
  "version": "0.0.1",
  "publisher": "esamatti",
  "repository": {
    "url": "https://github.com/epeli/vscode-unsaved"
  },
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:type"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.sayHello",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/lodash.debounce": "^4.0.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.4",
    "lodash.debounce": "^4.0.8",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6",
    "vscode": "^1.1.21"
  }
}
